TEST pcode_f8_to_f8_convert_Main()
{
	extern f8 pcode_f8_to_f8_convert(f8 a);
	ASSERTF8(pcode_f8_to_f8_convert((f8)0), (f8)0);
	ASSERTF8(pcode_f8_to_f8_convert(-(f8)0), -(f8)0);
	ASSERTF8(pcode_f8_to_f8_convert((f8)U8_MAGIC), (f8)U8_MAGIC);
	ASSERTF8(pcode_f8_to_f8_convert((f8)M_PI), (f8)M_PI);
	ASSERTF8(pcode_f8_to_f8_convert((f8)M_PIl), (f8)M_PIl);
	ASSERTF8(pcode_f8_to_f8_convert((f8)-M_PI), (f8)-M_PI);
	ASSERTF8(pcode_f8_to_f8_convert((f8)-M_PIl), (f8)-M_PIl);
}

