TEST unopPositivei1_Main()
{
	extern i1 unopPositivei1(i1 lhs);
	ASSERTI1(unopPositivei1(2), 2);
	ASSERTI1(unopPositivei1(I1_MAX), 127);
	ASSERTI1(unopPositivei1(I1_MIN), -128);
	ASSERTI1(unopPositivei1(0), 0);
}

TEST unopPositivei2_Main()
{
	extern i2 unopPositivei2(i2 lhs);
	ASSERTI2(unopPositivei2(0x0101), 0x0101);
	ASSERTI2(unopPositivei2(-0x0101), -0x0101);
	ASSERTI2(unopPositivei2(2), 2);
	ASSERTI2(unopPositivei2(I2_MAX), 32767);
	ASSERTI2(unopPositivei2(I2_MIN), -32768);
	ASSERTI2(unopPositivei2(0), 0);
}

TEST unopPositivei4_Main()
{
	extern i4 unopPositivei4(i4 lhs);
	ASSERTI4(unopPositivei4(0x01010101), 0x01010101);
	ASSERTI4(unopPositivei4(-0x01010101), -0x01010101);
	ASSERTI4(unopPositivei4(2), 2);
	ASSERTI4(unopPositivei4(I4_MAX), 2147483647);
	ASSERTI4(unopPositivei4(I4_MIN), -2147483648);
	ASSERTI4(unopPositivei4(0), 0);
}

TEST unopNegativei1_Main()
{
	extern i1 unopNegativei1(i1 lhs);
	ASSERTI1(unopNegativei1(2), -2);
	ASSERTI1(unopNegativei1(I1_MAX), -127);
	ASSERTI1(unopNegativei1(I1_MIN), -128);
	ASSERTI1(unopNegativei1(0), 0);
}

TEST unopNegativei2_Main()
{
	extern i2 unopNegativei2(i2 lhs);
	ASSERTI2(unopNegativei2(0x0101), -0x0101);
	ASSERTI2(unopNegativei2(-0x0101), 0x0101);
	ASSERTI2(unopNegativei2(I2_MAX), -32767);
	ASSERTI2(unopNegativei2(I2_MIN), I2_MIN);
	ASSERTI2(unopNegativei2(0), 0);
}

TEST unopNegativei4_Main()
{
	extern i4 unopNegativei4(i4 lhs);
	ASSERTI4(unopNegativei4(0x01010101), -0x01010101);
	ASSERTI4(unopNegativei4(-0x01010101), 0x01010101);
	ASSERTI4(unopNegativei4(I4_MAX), -I4_MAX);
	ASSERTI4(unopNegativei4(I4_MIN), I4_MIN);
