	ASSERTI1(pcode_i8_to_i1_convert(-U1_55), -U1_55);
	ASSERTI1(pcode_i8_to_i1_convert(-U1_AA), U1_56);
	ASSERTI1(pcode_i8_to_i1_convert(-U2_55), -U1_55);
	ASSERTI1(pcode_i8_to_i1_convert(-U2_AA), U1_56);
	ASSERTI1(pcode_i8_to_i1_convert(-U4_55), -U1_55);
	ASSERTI1(pcode_i8_to_i1_convert(-U8_55), -U1_55);
