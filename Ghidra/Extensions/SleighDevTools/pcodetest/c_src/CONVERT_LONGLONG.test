	ASSERTI1(pcode_i8_to_i1_convert(I1_MAX), I1_MAX);
	ASSERTI1(pcode_i8_to_i1_convert(I1_MIN), I1_MIN);
	ASSERTI1(pcode_i8_to_i1_convert(U1_MAX), -1);
}

